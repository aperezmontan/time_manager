<%= form_with model: new_project, class: "mb-7" do |form| %>
  <div class="mb-5">
    <%= form.text_field :name, placeholder: "Add a new project...", class: "shadow rounded-md border border-gray-200 outline-none px-3 py-2 mt-2 w-96" %>
    <%= form.submit "Add", class: "btn py-2 ml-2 bg-blue-600 text-white" %>
  </div>
<% end %>
